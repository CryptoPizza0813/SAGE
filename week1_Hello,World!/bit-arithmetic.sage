x, y = 0b1101, 0b1010
print ("x = {:04b}, y = {:04b}".format(x, y))
print ("x = {:04b}".format(x>>1))
print ("y<<1 = {:04b}".format(y<<1))
print ("x^^y = {:04b}".format(x^^y))
print ("x|y = {:04b}".format(x|y))
print ("x&y = {:04b}".format(x&y))
